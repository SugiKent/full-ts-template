{
  "name": "full-ts-with-ai-template",
  "version": "1.0.0",
  "description": "Full-Stack TypeScript Application Template with AI Development Support",
  "type": "module",
  "engines": {
    "node": ">=24.11.1",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\" --names \"server,client\" --prefix-colors \"cyan,magenta\"",
    "dev:all": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\" \"pnpm run worker:test\" --names \"server,client,worker\" --prefix-colors \"cyan,magenta,yellow\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/index.ts",
    "worker:test": "tsx watch src/server/workers/test-worker.ts",
    "worker:test:start": "tsx src/server/workers/test-worker.ts",
    "build": "tsc --noEmit && vite build",
    "start": "tsx src/server/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write src tests prisma customLinter && tsx customLinter/index.ts",
    "format": "biome format --write src tests prisma customLinter",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:testData": "tsx prisma/testData.ts",
    "db:studio": "prisma studio",
    "generate:og": "tsx og-images/generate.ts",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.33.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.0.2",
    "@orpc/client": "^1.13.0",
    "@orpc/server": "^1.13.0",
    "@prisma/client": "^6.19.1",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "bee-queue": "^2.0.0",
    "better-auth": "^1.4.7",
    "dotenv": "^17.2.3",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.1.0",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "nodemailer": "^7.0.11",
    "pino": "^10.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.5.1",
    "react-router-dom": "^7.11.0",
    "resend": "^6.6.0",
    "zod": "^4.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "biome check --write"
    ],
    "*.{json}": [
      "biome format --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "knip": "^5.79.0",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "playwright-core": "^1.57.0",
    "postcss": "^8.5.6",
    "prisma": "^6.19.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
